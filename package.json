{
  "name": "les-recouvrements-de-sols-andre-lavallee",
  "description": "Website for Les recouvrements de sols André Lavallée Inc.",
  "version": "1.0.0",
  "author": "Marc-Andre Bourassa",
  "private": "true",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "^4.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "gatsby": "^2.24.69",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-advanced-sitemap": "^1.5.6",
    "gatsby-plugin-fontawesome-css": "^1.0.0",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-netlify": "^2.3.17",
    "gatsby-plugin-netlify-cms": "^4.3.16",
    "gatsby-plugin-netlify-cms-paths": "^1.3.0",
    "gatsby-plugin-postcss": "^2.3.13",
    "gatsby-plugin-react-helmet": "^3.3.13",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.6.40",
    "gatsby-remark-images": "^3.3.33",
    "gatsby-source-filesystem": "^2.3.33",
    "gatsby-transformer-json": "^2.4.14",
    "gatsby-transformer-remark": "^2.8.38",
    "gatsby-transformer-sharp": "^2.5.17",
    "netlify-cms-app": "^2.12.25",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.9.2"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "9.8.6",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "prettier": "^2.1.2",
    "tailwindcss": "^1.8.12"
  }
}
